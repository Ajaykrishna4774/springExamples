package p;

class MyThread extends Thread
{
	@Override
	public void run() {
		// TODO Auto-generated method stub
		super.run();
		for(int i=0;i<=10;i++)
		{
			System.out.println(i);
		}
	}
}
public class ThreadExample {
public static void main(String[] args) {
	static MyThread t1=new MyThread();
	t1.start();
	t1.sleep(1000);
	
}
}
