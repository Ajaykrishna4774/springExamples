package com;
import java.io.FileInputStream;
public class file2 {
	public static void main(String args[]){    
         try{    
      	   FileOutputStream fout=new FileOutputStream("E:\\p1.txt");    
           fout.write(65);    
           fout.close();    
           System.out.println("success...");    
          }catch(Exception e){System.out.println(e);}

}
}
